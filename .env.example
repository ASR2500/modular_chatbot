# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
EMBEDDING_MODEL=text-embedding-3-small

# Database Configuration
CHROMA_PERSIST_DIR=./chroma_db
COLLECTION_NAME=python_faq_collection

# Data Configuration
DATA_PATH=./data/pythonfaq.csv

# RAG Configuration
DEFAULT_N_RESULTS=3
MAX_CONTEXT_LENGTH=4000

# Generation Configuration
MAX_TOKENS=800
TEMPERATURE=0.3

# Query Expansion
ENABLE_QUERY_EXPANSION=true
QUERY_EXPANSION_NUM_QUERIES=3
QUERY_EXPANSION_TEMPERATURE=0.7
QUERY_EXPANSION_MAX_TOKENS=500
QUERY_EXPANSION_STRATEGIES=synonyms,specificity,python_specific

# HyDE Configuration
ENABLE_HYDE=true
HYDE_MODEL=gpt-4o-mini
HYDE_TEMPERATURE=0.2
HYDE_MAX_TOKENS=150
HYDE_ANSWER_STYLE=concise
HYDE_INCLUDE_EXAMPLES=true
HYDE_BATCH_MODE=false
HYDE_BATCH_SIZE=3
HYDE_DOMAIN_CONTEXT=Answer as a Python expert
HYDE_ENABLE_CACHE=true
HYDE_CACHE_SIZE=100

# HyQE Configuration
ENABLE_HYQE=true
HYQE_MODEL=gpt-4o-mini
HYQE_QUESTIONS_PER_CHUNK=3
HYQE_QUESTION_STYLE=natural
HYQE_QUALITY=balanced
HYQE_TEMPERATURE=0.4
HYQE_MAX_TOKENS=100
HYQE_PREPEND_SYSTEM_PROMPT=true
HYQE_BATCH_MODE=true
HYQE_BATCH_SIZE=5
HYQE_MAX_CONCURRENT_REQUESTS=3
HYQE_INCLUDE_CHUNK_SUMMARY=true
HYQE_FALLBACK_TO_CHUNK_TEXT=true
HYQE_VERBOSE=false
HYQE_ENABLE_CONTENT_HASHING=true
HYQE_SKIP_UNCHANGED_CONTENT=true
HYQE_ENABLE_CACHE=true
HYQE_CACHE_SIZE=200
HYQE_EMBEDDING_MODEL=text-embedding-3-small
HYQE_MIN_QUESTION_LENGTH=10
HYQE_MAX_QUESTION_LENGTH=200
HYQE_DUPLICATE_THRESHOLD=0.85

# NER Configuration
ENABLE_NER=true
NER_MODEL=en_core_web_sm
NER_CONFIDENCE_THRESHOLD=0.5
